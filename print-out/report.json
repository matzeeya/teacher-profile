        "columns": [
          {"width": 150,"text": "เริ่มรับราชการเมื่อวันที่"},
          {"stack": [{"width": 80,"text": "{{start_date}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 80,"y2": 0,"lineWidth": 0.75}]}]
          },
          {"width": 50,"text": "ตำแหน่ง"},
          {"stack": [{"width": 150,"text": "{{rank}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 150,"y2": 0,"lineWidth": 0.75}]}]
          }
        ],"margin": [0,1,0,1]
      },
      {
        "columns": [
          {"width": 80,"text": "โรงเรียน"},
          {"stack": [{"width": 80,"text": "{{first_school.hostname}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 80,"y2": 0,"lineWidth": 0.75}]}]
          },
          {"width": 150,"text": "สำนักงานเขตพื้นที่การศึกษา"},
          {"stack": [{"width": 150,"text": "{{first_school.area}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 150,"y2": 0,"lineWidth": 0.75}]}]
          }
        ],"margin": [0,1,0,1]
      },
      {
        "columns": [
          {"width": 50,"text": "อำเภอ"},
          {"stack": [{"width": 150,"text": "{{first_school.amphoe}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 150,"y2": 0,"lineWidth": 0.75}]}]
          },
          {"width": 50,"text": "จังหวัด"},
          {"stack": [{"width": 150,"text": "{{first_school.province}}"},
            {"canvas": [{"type": "line","x1": 0,"y1": 0,"x2": 150,"y2": 0,"lineWidth": 0.75}]}]
