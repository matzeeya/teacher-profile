            {"canvas": [{"type": "line","x1": -1,"y1": 0,"x2": 100,"y2": 0,"lineWidth": 0.75}]}]
          }
        ],"margin": [0,1,0,1]
      },
      {
        "columns": [
          {"width": 150,"text": "วิทยฐานะ"},
          {"stack": [{"width": 100,"text": "{{career_title}}"},
            {"canvas": [{"type": "line","x1": -1,"y1": 0,"x2": 100,"y2": 0,"lineWidth": 0.75}]}]
          }
