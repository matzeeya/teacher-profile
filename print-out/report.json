              {{#jobs}}
              {
                "columns": [
                  {"stack": [{"width": 210,"text": "{{approve.list}}"}]}
                ],"margin": [0,1,0,1]
              }
              {{/jobs}},
              {{#jobs}}
              {
                "columns": [
                  {"stack": [{"width": 180,"text": "{{subject}}"}]}
                ],"margin": [0,1,0,1]
              }
              {{/jobs}}
              ]{{#comma}},{{/comma}}          
            {{/teaching_class}}
          ]
        },"margin": [0,10,0,20]
