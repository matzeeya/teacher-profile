            ["{{year}}","{{work_special}}","{{start_date}}","{{end_date}}"]{{#comma}},{{/comma}}          
            {{/operating_special}}
          ]
        },"margin": [0,10,0,20]
      },
      {"text": "การจัดการเรียนการสอน","bold": true, "fontSize": 18,"margin": [0, 10, 0, 0]},
