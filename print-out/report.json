      {
        "table": {
          "widths": [740],
          "headerRows":1,
          "widths":[120,250,250,120],
          "body": [
            ["ปีการศึกษา","หน่วยงานที่จัดอบรม/สัมมนา","หัวข้อที่อบรม/สัมมนา","ระยะเวลา(ชั่วโมง)"],
            {{#training}}
            ["{{year}}","{{institution_training}}","{{topic}}","{{hour}}"]{{#comma}},{{/comma}}          
            {{/training}}
          ]
        },"margin": [0,10,0,20]
      },
      {"text": "การเป็นวิทยากร","margin": [0, 0, 0, 0]},
      {
        "table": {
          "widths": [740],
          "headerRows":1,
          "widths":[80,80,160,150,160,80],
          "body": [
