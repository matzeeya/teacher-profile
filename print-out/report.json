            {"canvas": [{"type": "line","x1": -1,"y1": 0,"x2": 170,"y2": 0,"lineWidth": 0.75}]}]
          },
          {"width": 100,"text": "ประสบการณ์การสอน"},
          {"stack": [{"width": 50,"text": "{{experience}} ปี"},
            {"canvas": [{"type": "line","x1": -1,"y1": 0,"x2": 50,"y2": 0,"lineWidth": 0.75}]}]
          }
        ],"margin": [0,1,0,1]
      },
      {"text": "ประวัติการทำงาน","bold": true, "fontSize": 18,"margin": [0, 10, 0, 0]},
