      {
        "table": {
          "widths": [740],
          "headerRows":1,
          "widths":[100,150,250,250],
          "body": [
            ["ปีการศึกษา","ชื่อ-นามสกุล นักเรียน","รางวัลที่ได้รับ","หน่วยงานที่มอบรางวัล"],
            {{#teaching_reward}}
            ["{{year}}","{{student_name}}","{{reward}}","{{institution_reward}}"]{{#comma}},{{/comma}}          
            {{/teaching_reward}}
          ]
        },"margin": [0,10,0,20]
